<h1>Listing users</h1>

<table>
  <tr>
    <th>User</th>
    <th></th>
  </tr>
  
<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.avatar %></td>
    <td><%= link_to profile.user.username, profile %></td>
  </tr>
<% end %>
</table>

<br />
<div id="footer">
<%= link_to 'Back to your dashboard', dashboards_path %>
</div>